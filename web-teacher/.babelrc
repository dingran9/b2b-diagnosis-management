{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 5 versions", "not ie <= 8"]
      }
    }],
    "stage-0"
  ],
  "plugins": ["transform-runtime", "transform-object-assign"],
  "env": {
    "test": {
      "presets": ["env", "stage-0"],
      "plugins": ["istanbul", "transform-object-assign"]
    }
  }
}
